{
    "name": "RandImgur",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Generate a random Imgur URL and roll the dice!",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon.png"
    },
    "permissions": [
        "tabs",
        "http://api.imgur.com/"
    ]
}
